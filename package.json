  "private": true,
  "scripts": {
    "dev": "yarn nx serve api-gateway",
    "msa:build": "yarn tsc libs/msa-runner/src/lib/msa-runner.ts",
    "msa:run": "yarn node libs/msa-runner/src/lib/msa-runner.js",
    "nx:run-many": "yarn nx run-many --target=serve --all --parallel",
    "nx:reset": "yarn nx reset",
    "nx:add:nest:app": "yarn nx generate @nx/nest:app",
    "nx:add:nest:lib": "yarn nx generate @nx/nest:lib",
    "nx:add:js:lib": "yarn nx generate @nx/js:lib",
    "nx:remove": "yarn nx generate remove",
    "db:pull": "yarn prisma db pull",
    "db:push": "yarn prisma db push",
    "db:build": "yarn prisma generate"
  },
  "prisma": {
    "schema": "libs/prisma-schema/src/lib/prisma/schema.prisma"
  },
